{
  "name": "aws-starter-comprehend",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "aws-starter-comprehend": "./bin/CloudFormation.js"
  },
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.ts",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.ts --watch --progress",
    "test": "jest",
    "cdk": "cdk",
    "submit": "node -e 'require(\"./bin/delivery-handler/index.js\").main()'",
    "get": "node -e 'require(\"./bin/post-handler/index.js\").main()'"
  },
  "devDependencies": {
    "@aws-cdk/assets": "^1.3.0 ",
    "@aws-cdk/aws-apigateway": "^1.3.0 ",
    "@aws-cdk/aws-cloudwatch": "^1.3.0 ",
    "@aws-cdk/aws-cloudtrail": "^1.3.0 ",
    "@aws-cdk/aws-ec2": "^1.3.0 ",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.3.0 ",
    "@aws-cdk/aws-elasticsearch": "^1.3.0 ",
    "@aws-cdk/aws-iam": "^1.3.0 ",
    "@aws-cdk/aws-kinesis": "^1.3.0 ",
    "@aws-cdk/aws-lambda": "^1.3.0 ",
    "@aws-cdk/aws-lambda-event-sources": "^1.3.0 ",
    "@aws-cdk/aws-s3": "^1.3.0 ",
    "@aws-cdk/aws-sqs": "^1.3.0 ",
    "@aws-cdk/aws-sns": "^1.3.0 ",
    "@aws-cdk/core": "^1.3.0 ",
    "@types/jest": "^24.0.11",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/webpack": "^4.4.26",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/zip-webpack-plugin": "^3.0.0",
    "archiver": "^3.0.0",
    "aws-cdk": "^1.3.0 ",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "json-loader": "^0.5.7",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "zip-webpack-plugin": "^3.0.0"
  }
